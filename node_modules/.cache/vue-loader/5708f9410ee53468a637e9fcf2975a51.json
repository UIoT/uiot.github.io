ems class=\"pr-10\">\n        <v-btn text to=\"/publications\"  :class=\"{ base: checkRouteColor('publications', currentRoute), 'lighten-2': true }\">Publications</v-btn>\n        <v-btn text to=\"/projects\"      :class=\"{ base: checkRouteColor('projects', currentRoute), 'lighten-2': true }\">Projects</v-btn>\n        <v-btn text to=\"/team\"          :class=\"{ base: checkRouteColor('team', currentRoute), 'lighten-2': true }\">Team</v-btn>\n        <v-menu offset-y>\n          <template v-slot:activator=\"{ on }\">\n            <v-btn\n              text\n              v-on=\"on\"\n            >\n              {{currentLang}}\n            </v-btn>\n          </template>\n          <v-list>\n            <v-list-item\n              v-for=\"(item, index) in languages\"\n              :key=\"index\"\n            >\n              <v-list-item-title >\n                <v-btn text @click=\"changeLocale(item.language)\" class=\"botao\">\n                  <v-img width=\"30\" :src=\"require(`@/assets/${item.img}`)\"></v-img> <v-span class=\"lang-title\"> {{ item.title }} </v-span>\n                  \n                </v-btn>\n              </v-list-item-title>\n            </v-list-item>\n          </v-list>\n    </v-menu>\n      </v-toolbar-items>\n\n    </v-toolbar>\n  </div>\n</template>\n\n<script>\nimport i18n from '@/plugins/i18n';\n\nexport default {\n  data() {    \n    return {      \n      items: [\n        { title: 'English' },\n        { title: 'Portugues' },\n      ],  \n      languages: [            \n        { \n          img: 'united-states.png',\n          flag: 'us', \n          language: 'en', \n          title: 'English' \n        },            \n        { \n          img: 'brazil.png',\n          flag: 'pt-br', \n          language: 'pt-br', \n          title: 'Portugues' \n        }        \n      ],\n      currentLang: \"en\",    \n    };\n  },\n  computed: {\n    currentRoute(){\n      return this.$route.name\n    }\n  },\n  methods: {    \n    changeLocale(locale) {        \n      i18n.locale = locale;\n      this.currentLang = i18n.locale    \n    },\n    checkRouteColor(name, routeName){\n      if(name == routeName){\n        return true\n      }else{\n        return false\n      }\n    },\n    goTo(){\n      this.$router.push(\"/\")\n    }\n  }\n}\n</script>\n\n<style scoped>\n.nameLogo{\n  float: left;\n}\n\n.titleText{\n  padding-left: 10px;\n}\n\n.rotateLogo:hover{\n  transform: rotate(360deg);\n  transition: 2s;\n  \n}\n\n.toolbar{\n    font-family: 'Titillium Web', sans-serif;\n}\n\n.botao{\n    font-family: 'Raleway', sans-serif;\n}\n\n.hoverItem{\n  background-color: blue;\n}\n\n.style-top{\n  height: 10px;\n}\n</style>"]}]}